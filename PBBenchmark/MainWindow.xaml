<Window x:Class="PBBenchmark.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Canvas>
        <TextBox Height="252" HorizontalAlignment="Left" Margin="12,47,0,0" Name="textBox1" VerticalAlignment="Top" Width="240" HorizontalScrollBarVisibility="Hidden" VerticalScrollBarVisibility="Visible" />
        <Button Content="Open Workspace" Height="23" HorizontalAlignment="Left" Margin="12,12,0,0" Name="button1" VerticalAlignment="Top" Width="117" Click="button1_Click" />
        <TextBox Canvas.Left="258" Canvas.Top="47" Height="252" Name="textBoxResult" Width="233" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Visible" />
        <Button Canvas.Left="135" Canvas.Top="12" Content="Open Workspace (Thread)" Height="23" Name="button2" Width="194" Click="button2_Click" />
    </Canvas>
</Window>
